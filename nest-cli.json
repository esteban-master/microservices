  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mantainer/src",
  "monorepo": true,
  "root": "apps/mantainer",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/mantainer/tsconfig.app.json"
